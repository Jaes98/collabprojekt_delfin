rest

linje 80

async function updateMemberPatch(updatedMember, id) {
  const objectToJSON = JSON.stringify(updatedMember);

  const response = await fetch(`${dolphinDatabase}/members/${id}.json`, {
    method: "PATCH",
    body: objectToJSON
  });
  if (response.status === 200) {
    console.log("****************200***************");
    successPrompt();
  } else {
    console.log("################shit#############");
    failedPrompt();
  }

  return response;
}

